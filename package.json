{
  "name": "rest-mock-json",
  "version": "1.0.1",
  "description": "Mock express API powered by json",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.ts --extensions \".ts\" -- -f ./hide/foo.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir bin --extensions \".ts,.tsx\" --source-maps inline",
    "prepublish": "npm run build"
  },
  "keywords": [
    "mock",
    "api",
    "express",
    "server",
    "backend",
    "frontend",
    "test",
    "router",
    "json"
  ],
  "author": "Anthony Freda <anthonyfreda323@gmail.com>",
  "license": "ISC",
  "bin": {
    "rest-mock-json": "./bin/index.js"
  },
  "preferGlobal": true,
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/body-parser": "^1.17.0",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/node": "^11.11.3",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "cors": "^2.8.5",
    "express": "^4.16.4"
  }
}
